{
    "manifest_version": 3,
    "name":"Entity Highlighter",
    "description": "Highlight all entities in a browser",
    "version" : "1.0",
    "icons":{
        "16":"/icons/icon16-2.png",
        "48": "/icons/icon48-2.png",
        "128": "/icons/icon128-2.png"
    },
    "action": {
        "default_title": "Entity Highlighter",
        "default_icon": {
            "16":"/icons/icon16-2.png",
            "48": "/icons/icon48-2.png",
            "128": "/icons/icon128-2.png"
        }
    },
    "content_scripts":[{
        "js": ["/static/content.js"],
        "matches": ["<all_urls>"]
    }],
    "background":{
        "service_worker": "background.js"
        
    }
}


